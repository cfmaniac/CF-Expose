<cfSilent>	<cfScript>		vars=structNew();		//Get the factory		f = ormGetSessionFactory();		fStats = f.getStatistics();		//Enable stats		fStats.setStatisticsEnabled(javacast("boolean",true));		//Get the session		s = ormGetSession();		//Get the stats for the session		sessionStats = s.getStatistics();		//DSN		dsn = s.connection().getCatalog();		//Type		type = s.connection().getMetaData().getDatabaseProductName();		//Version		version = s.connection().getMetaData().getDatabaseProductVersion();		s.disconnect();	</cfScript>	<cfoutput>	DSN=#dsn# (#type# #version#)<p>	</cfoutput>	<cfScript>		entityCount = sessionStats.getEntityCount();		entities = sessionStats.getEntityKeys().toArray();		niceEntities = [];		for(local.eIndex=arrayLen(entities); local.eIndex GT 0; --local.eIndex)		{			e = entities[local.eIndex];			name = e.getEntityName();			id = e.getIdentifier();			arrayAppend(niceEntities, {name=name, id=id});		}	</cfScript>	<cfdump var="#niceEntities#" label="Entities in this request" />	<cfScript>		totalStats=		{			inserts = fStats.getEntityInsertCount()			,loads = fStats.getEntityLoadCount()			,deletes = fStats.getEntityDeleteCount()			,updates = fStats.getEntityUpdateCount()			,fetches = fStats.getEntityFetchCount()			,loads = fStats.getEntityLoadCount()			,hql = fStats.getQueries()			,entities = structNew()		};		local.nameArray = fStats.getEntityNames();		for(local.eIndex=arrayLen(local.nameArray); local.eIndex GT 0; --local.eIndex)		{			n = local.nameArray[local.eIndex];			stats = fStats.getEntityStatistics(n);			totalStats.entities[n]=			{				 deletes = stats.getDeleteCount()				,inserts = stats.getInsertCount()				,updates = stats.getUpdateCount()				,loads = stats.getLoadCount()			};		}		totalStats.flushcount = fStats.getFlushCount();		totalStats.sessions = fStats.getSessionOpenCount();		totalStats.transactions = fStats.getTransactionCount();		vars.output = totalStats;	</cfScript>	<cfif structKeyExists( attributes , "returnVariable" ) >		<cfParam name="attributes.returnVariable" type="variableName" />		<cfset "caller.#attributes.returnVariable#" = vars.output />		<cfExit />	</cfif></cfSilent><cfOutput>#vars.output#</cfOutput><cfExit method="exitTag">